# API Keys
# 실제 사용 시 이 파일을 .env로 복사하고 실제 키 값을 입력하세요

# DART API Key (https://opendart.fss.or.kr)
DART_API_KEY=your_dart_api_key_here

# Gemini API Key (https://aistudio.google.com/app/apikey)
GEMINI_API_KEY=your_gemini_api_key_here

# Friendli AI Settings (https://friendli.ai/docs/guides/dedicated_endpoints/quickstart)
FRIENDLI_TOKEN=your_friendli_token_here
FRIENDLI_ENDPOINT_ID=your_endpoint_id_here

# Flask Server Settings (Optional)
FLASK_DEBUG=True
FLASK_HOST=0.0.0.0
FLASK_PORT=5000

# Vector DB Settings (Optional)
VECTOR_DB_DIR=vector_db

# Logging (Optional)
LOG_LEVEL=INFO

